  "description": "EventDispatcher written in ES2016",
  "main": "index.js",
  "scripts": {
    "start": "yarn build",
    "esdoc": "cd ./babels && esdoc -c esdoc.json",
    "check": "jscheckstyle --html ./libs/moku.js > ./babels/check/jscheckstyle.html",
