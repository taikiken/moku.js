  "license": "MIT",
  "homepage": "",
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "browser-sync": "^2.18.13",
    "core-js": "^2.5.7",
    "del": "^3.0.0",
    "esdoc": "^1.1.0",
